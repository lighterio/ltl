{
  "name": "ltl",
  "version": "0.1.15",
  "description": "Lean Template Language for JavaScript and HTML",
  "dependencies": {},
  "devDependencies": {
    "coffee-script": "1.8.0",
    "coveralls": "2.11.1",
    "dot": "1.0.2",
    "exam": "^0.0.8",
    "istanbul": "0.3.1",
    "jade": "1.6.0",
    "markdown": "0.5.0",
    "marked": "0.3.2",
    "zeriousify": "^0.1.10"
  },
  "keywords": [
    "lean",
    "lightweight",
    "fast",
    "simple",
    "template",
    "engine",
    "jade",
    "dot"
  ],
  "main": "ltl",
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "perf": "node perf"
  },
  "repository": "https://github.com/lighterio/ltl.git",
  "bugs": "https://github.com/lighterio/ltl/issues",
  "homepage": "http://lighter.io/ltl",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>",
    "Kaushik Shankar <skaushik92@gmail.com>"
  ],
  "license": "MIT"
}
