    "istanbul": "0.3.1",
    "jade": "1.6.0",
    "markdown": "0.5.0",
    "marked": "0.3.2",
    "zeriousify": "0.1.9"
  },
  "bugs": {
    "url": "http://github.com/lighterio/ltl/issues"
  },
  "licenses": [
